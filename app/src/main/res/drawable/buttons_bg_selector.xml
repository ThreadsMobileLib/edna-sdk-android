<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/black_color_2d"/>
            <corners
                android:radius="12dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid
                android:color="@color/gray_color_f4"/>
            <corners
                android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/black_color"/>
            <corners
                android:radius="12dp" />
        </shape>
    </item>
</selector>